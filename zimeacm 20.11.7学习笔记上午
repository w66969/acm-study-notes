public class flywithjavaandxz
{
   public static void main(String[] args)
   {
		for(int i=1;i<=60;i++)
		{
			for(int j=1;j<=i;j++)
			{
				System.out.print(" ");
			}
			
			System.out.print("Java带你飞\r");

			String hehe="";
			for(int k=1;k<=10000;k++)
			{
				hehe=hehe+k;
			}
			
		}
    }
}




public class huiche
{
	public static void main(String[] args)
	{
		
		for(int i=1;i<=10;i=i+1)
		{
			for(int k=1;k<=10-i;k++)
			{
				System.out.print(" ");
			}
			for(int j=1;j<=2*i-1;j++)
			{
				System.out.print("*");
			}
			System.out.println();
		}
		
		
	}
}



public class test
{

	public static void main(String[] args)
	{
		for(int i=1;i<=11;i++)
		{
			System.out.printf("%05d\n",i);
			//%5d   %-5d  %.2f
		}
	}

}


import java.util.Scanner;


public class ninenine
{
	public static void main(String[] args)
	{
		int n=0;
		Scanner s=new Scanner(System.in);
		while(s.hasNext())
		{
			n=s.nextInt();
			{
				for(int i=1;i<=n;i++)
				{
					for(int j=1;j<=i;j++)
					{
						if(j==i)
						{
							
							System.out.printf("%2d *%2d = %2d",i,j,(i*j));
						}
						else
						{
							System.out.printf("%2d *%2d = %2d\t",i,j,(i*j));
						}
					}
					System.out.println();
				}
				System.out.println();
			}
		}

	}
}


import java.util.Scanner;


public class caishengxiao
{

	public static void main(String[] args)
	{
		String[] zidian={"konglong","rat", "ox", "tiger","rabbit", "dragon" ,"snake","horse","goat","monkey","rooster","dog","pig","dinosaur"};
		String code="";
		Scanner s=new Scanner(System.in);
		while(s.hasNext())
		{
			code=s.next();//空格分开的或者回车分开的字符串。  nextLine();//这个可以输入空格
			System.out.println(zidian[Integer.parseInt(code, 2)]);
		}

	}

}



